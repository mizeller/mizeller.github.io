<head>
  <script defer src="https://cloud.umami.is/script.js" data-website-id="f38e5e8c-a760-4c0a-814c-c1489633d62f"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/three@0.128.0/examples/js/loaders/OBJLoader.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/three@0.128.0/examples/js/controls/OrbitControls.js"></script>
  <meta content="text/html; charset=UTF-8" name="Content-Type"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
  <meta name="viewport" content="width=device-width"/>
  <meta property="og:site_name" content="{{ title }}"/>
  <meta property="og:locale" content="en_US"/>
  <meta property="og:title" content="{{ title }}"/>
  <meta property="og:type" content="article"/>

  {% if description %}
    <meta property="og:description" content="{{ description }}"/>
    <meta name="description" content="{{ description }}"/>
  {% else %}
    <meta name="description" content="Hello World by Michel Zeller"/>
    <meta property="og:description" content="Hello World by Michel Zeller"/>
  {% endif %}

  <meta property="og:image" content="/assets/nxt.png"/>
  <meta name="author" content="Michel Zeller"/>

  {% if permalink %}
    <meta property="og:url" content="https://mizeller.github.io/{{ permalink }}"/>
    <link rel="canonical" href="https://mizeller.github.io/{{ permalink }}"/>
  {% else %}
    <meta property="og:url" content="https://mizeller.github.io/"/>
    <link rel="canonical" href="https://mizeller.github.io/"/>
  {% endif %}

  <link rel="icon" href="{{ '/assets/nxt.png' | url }}" sizes="32x32">
  <link rel="icon" href="{{ '/assets/nxt.png' | url }}" sizes="192x192">
  <link rel="icon" href="{{ '/assets/nxt.ico' | url }}" type="image/x-icon"/>
  <title>{{ title }} - Michel Zeller</title>
  <link rel="stylesheet" href="{{ '/css/styles.css' | url }}">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
</head>